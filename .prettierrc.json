{
    "printWidth": 80,
    "tabWidth": 4,
    "trailingComma": "all",
    "singleQuote": true,
    "semi": true,
    "importOrder": [
        "^@/styles/(.*)$",
        "^(react/(.*)$)|^(react$)",
        "^(next/(.*)$)|^(next$)",
        "^@nextui-org/(.*)$",
        "",
        "^@/components/(.*)$",
        "^@/app/(.*)$",
        "^@/config/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
